    <body bgcolor="#F0F8FE">
        <form action="hospitalregistration">
        
         <center>
                <h1>Hospital Registration Form</h1>
                <table class="table table-bordered table-bordered" style="width:50%;text-align:left">
                    <tr>
                        <td>
                            <b>Hospital Name:</b>
                        </td
                        >
                        <td>
                            <input class="form-control" type="text" name="hname">
                        </td>
                    </tr>
                    
                     <tr>
                        <td>
                            <b>Address of Hospital:</b>
                        </td>
                        <td>
                            <input class="form-control" type="text" name="hadd">
                        </td>
                   
                                      
                    
                         
                   <tr>
                        <td>
                         <b> City: </b>
                        </td> 
                        <td>
                         <input class="form-control" type="text" name="city">
                        </td>
                   </tr> 
                   
                      
                   <tr>
                        <td>
